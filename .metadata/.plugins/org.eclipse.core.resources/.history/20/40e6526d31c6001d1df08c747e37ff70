#ifndef DC_MOTOR_CONFIG_H_
#define DC_MOTOR_CONFIG_H_


#define DC_MOTOR_SWITCH 0

//DC MOTOR Driver Port
#define DC_MOTOR_PORT		DC_MOTOR_PORTB

//DC MOTOR Driver Pins
